    <div class="login-container">
        <!-- Asegúrate de que la ruta de la imagen sea correcta -->
        <img src="~/Imagenes/Logo.png" alt="Logo" class="login-logo" />
        <h2>Inicie sesión</h2>
        <form asp-action="Login" method="post">
            <div class="form-group">
                <input type="email" class="form-control" id="Email" name="Email" placeholder="Correo electrónico" required />
            </div>
            <div class="form-group">
                <input type="password" class="form-control" id="Password" name="Password" placeholder="Contraseña" required />
            </div>
            <button type="submit" class="btn btn-primary">Ingresar</button>
            @if (!ViewData.ModelState.IsValid)
            {
                <div class="input-toast">
                    @foreach (var error in ViewData.ModelState.Values.SelectMany(v => v.Errors))
                    {
                        <span>@error.ErrorMessage</span>
                    }
                </div>
            }
        </form>
    </div>

    <script>
        // Aseguramos que la animación se reajuste al redimensionar la ventana
        var animatedMarquee = $('.animated-marquee');
        $(window).resize(function () {
            animatedMarquee.remove();
            $('body').append(animatedMarquee);
        });
    </script>
    <script src="~/js/sesionkiller.js"></script>
</body>
</html>
